{
  "name": "github-deploy-center",
  "private": true,
  "scripts": {
    "analyze": "yarn build && open stats.html",
    "start": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "codegen": "graphql-codegen --config codegen.yml",
    "update-github-schema": "$GITHUB_PAT && curl -H \"Authorization: Bearer $GITHUB_PAT\" https://api.github.com/graphql --output github.schema.json && json-format github.schema.json --indent 2",
    "package": "mkdir -p artifacts && cd dist && zip -r ../artifacts/release.zip ."
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.12",
    "@octokit/rest": "^19.0.7",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-query-devtools": "^4.26.1",
    "browser-fs-access": "^0.33",
    "dayjs": "^1.11.7",
    "fp-ts": "^2.13.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "graphql-tag": "^2.12.6",
    "io-ts": "^2.2.20",
    "isomorphic-fetch": "^3.0.0",
    "lodash-es": "^4.17.21",
    "overmind": "^28.0.2",
    "overmind-react": "^29.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal-promise": "^1.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "3.0.2",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.11",
    "json-format": "^1.0.1",
    "json-format-cli": "^1.1.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}